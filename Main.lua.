-- Delta Specific White Cat Hub
local CoreGui = game:GetService("CoreGui")

-- Check if old UI exists and delete it
if CoreGui:FindFirstChild("WhiteCatDelta") then
    CoreGui.WhiteCatDelta:Destroy()
end

local screen = Instance.new("ScreenGui")
screen.Name = "WhiteCatDelta"
screen.Parent = CoreGui

-- Special Delta Function to hide UI from Game Scripts
if syn and syn.protect_gui then
    syn.protect_gui(screen)
end

local frame = Instance.new("Frame", screen)
frame.Size = UDim2.new(0, 220, 0, 120)
frame.Position = UDim2.new(0.5, -110, 0.3, 0)
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
frame.Active = true
frame.Draggable = true

local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 20)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 1, 0)
title.Text = "üêæ White Cat Hub üêæ\nStatus: Online (Delta)"
title.TextColor3 = Color3.fromRGB(255, 182, 193)
title.Font = Enum.Font.GothamBold
title.TextSize = 18
title.BackgroundTransparency = 1

print("White Cat Hub injected via Delta!")
